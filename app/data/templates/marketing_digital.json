{
  "id": "marketing_digital",
  "name": "Digital Marketing Specialist",
  "category": "Marketing",
  "target_roles": ["Marketing Manager", "Digital Marketing", "Growth Marketing", "Marketing Specialist"],
  "experience_levels": ["Fresher", "Mid", "Senior"],
  "ats_optimized": true,
  "layout": {
    "columns": 1,
    "margins": {"top": 0.75, "bottom": 0.75, "left": 0.75, "right": 0.75},
    "spacing": {"section_gap": 12, "subsection_gap": 8, "line_height": 1.2}
  },
  "fonts": {
    "primary": {
      "family": "Calibri",
      "sizes": {"name": 22, "section_header": 12, "subsection_header": 11, "body": 10.5},
      "weights": {"name": "bold", "section_header": "bold", "subsection_header": "bold", "body": "normal"}
    }
  },
  "sections": {
    "order": ["personal_info", "summary", "experience", "skills", "education", "certifications"],
    "personal_info": {"layout": "centered", "fields": ["name", "email", "phone", "linkedin", "website"]},
    "summary": {"label": "Professional Summary", "max_lines": 3},
    "experience": {
      "label": "Professional Experience",
      "date_format": "MMM YYYY",
      "bullet_style": "disc",
      "highlight_keywords": ["campaign", "SEO", "SEM", "social media", "analytics", "conversion", "ROI", "growth", "content"]
    },
    "skills": {"label": "Skills & Tools", "format": "inline", "separator": " | "},
    "education": {"label": "Education", "show_gpa": "optional"},
    "certifications": {"label": "Certifications", "format": "inline"}
  },
  "styling": {
    "colors": {"primary": "#000000", "secondary": "#444444", "accent": "#0066cc"},
    "section_separator": {"type": "line", "thickness": 0.5, "color": "#cccccc"}
  }
}
